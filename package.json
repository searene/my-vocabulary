    "file-loader": "^6.1.1",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "html-webpack-plugin": "^5.0.0-alph.14",
    "husky": "^4.3.0",
    "image-webpack-loader": "^7.0.1",
    "jest": "^26.6.1",
    "less": "^3.12.2",
    "lint-staged": "^10.4.2",
