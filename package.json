    "html-to-text": "^5.1.1",
    "immutable": "^4.0.0-rc.12",
    "knex": "^0.21.6",
    "knex-stringcase": "^1.4.3",
    "less-loader": "^7.0.2",
    "node-addon-api": "*",
    "node-gyp": "^7.1.2",
    "react": "^17.0.1",
    "react-autosuggest": "^10.0.3",
